<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Trang chủ</title>
    <style><%@include file="/view/style.css"%></style>
</head>
<body>
<div class="header">
    <div class="banner">
        <img class="image"
             src="data:image/jpeg;base64,">
    </div>
    <div class="search">
        <input type="text" name="search" placeholder="Search">
        <button>Search</button>
    </div>
</div>
<div class="main">
    <div class="name-table">
        <h1>Danh sách sản phẩm</h1>
    </div>
    <table class="show-book">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
            <th>Description</th>
            <th>Author</th>
            <th>Image</th>
            <th colspan="2">Action</th>
        </tr>
        <c:forEach var="item" items="${bookList}">
            <tr>
                <td>${item.id}</td>
                <td>${item.name}</td>
                <td>${item.price}</td>
                <td>${item.description}</td>
                <td>${item.author}</td>
                <td><img src="${item.image}" class="image"></td>
                <td>
                    <button><a href="http://localhost:9999/books?action=edit&id=${item.id}">Edit</a></button>
                </td>
                <td>
                    <button><a href="http://localhost:9999/books?action=delete&id=${item.id}">Delete</a></button>
                </td>
            </tr>
        </c:forEach>
        <tr>
            <td>
                <a href="http://localhost:9999/books?action=create">Create Book</a>
            </td>
        </tr>
    </table>
</div>
</body>
</html>